{
  "name": "Numbers",
  "version": "0.0.1-alpha2",
  "description": "Board game with numbers",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf built && webpack-dev-server --config webpack.config.js",
    "build": "rm -rf built && webpack",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/test/setupTests.js"
    ],
    "moduleNameMapper": {
      "^@model/(.*)$": "<rootDir>/src/js/model/$1",
      "^@components/(.*)$": "<rootDir>/src/js/components/$1",
      "^@icons/(.*)$": "<rootDir>/src/js/icons/$1",
      "^@containers/(.*)$": "<rootDir>/src/js/containers/$1",
      "^@root/(.*)$": "<rootDir>/src/js/$1",
      "^@test/(.*)$": "<rootDir>/src/test/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosegon/numbers.git"
  },
  "keywords": [
    "casual",
    "game",
    "numbers"
  ],
  "author": "sosegon <anse23@hotmail.com> (https://github.com/sosegon)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sosegon/numbers/issues"
  },
  "homepage": "https://github.com/sosegon/numbers#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cheerio": "1.0.0-rc.12",
    "css-loader": "^6.11.0",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.7.4",
    "redux-mock-store": "^1.5.3",
    "resolve-url-loader": "^3.0.1",
    "style-loader": "^3.3.4",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "^5.104.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^6.1.19",
    "uuid": "^3.3.2"
  }
}
